{
  "plots": [
    {
      "x": "container_cnt",
      "x_lbl": "#Containers",
      "y": "throughput_lps",
      "y_lbl": "Throughput",
      "hue_lbl": "Executer",
      "hue": "execution_unit",
      "shape": "barplot",
      "title": "Throughput (lps) vs. #container"
    }
  ],
  "benchmark_config": {
    "duration": 3,
    "exec_env": [
      "native"
    ],
    "monitors": {
      "mpstat": [
        "-n",
        "-u",
        "-I",
        "SCPU,SUM",
        "-P",
        "0,1"
      ],
      "perf": [
        "-C",
        "0,1"
      ]
    },
    "threads": {
      "values": [
        [
          1
        ]
      ]
    }
  },
  "containers": {
    "container_list": {
      "values": [
        {
          "min": 1,
          "step": 1,
          "max": 3
        }
      ]
    },
    "core_count": 3,
    "image": "ubuntu:latest"
  },
  "applications": [
    {
      "path": "bm-external/byte-unixbench/UnixBench",
      "name": "Run",
      "args": "-c {threads} -i 1 -b -q syscall",
      "adapter": {
        "name": "unix-bench-adapter.sh"
      }
    }
  ]
}
